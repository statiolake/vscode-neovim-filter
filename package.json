{
  "name": "vscode-neovim-filter",
  "displayName": "Neovim Filter",
  "description": "Use Neovim as a powerful filter",
  "version": "0.1.1",
  "publisher": "statiolake",
  "license": "MIT",
  "engines": {
    "vscode": "^1.108.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/statiolake/vscode-neovim-filter"
  },
  "bugs": {
    "url": "https://github.com/statiolake/vscode-neovim-filter/issues"
  },
  "homepage": "https://github.com/statiolake/vscode-neovim-filter#readme",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "neovimFilter.runExCommand",
        "title": "Neovim Filter: Run ex command"
      },
      {
        "command": "neovimFilter.clearHistory",
        "title": "Neovim Filter: Clear command history"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "vsce package",
    "publish": "vsce publish",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "neovim": "^5.4.0"
  }
}
